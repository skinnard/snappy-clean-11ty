<!-- contact info -->
<div class="mil-p-0-f">
    <div class="container">
        <div class="mil-section-title mil-mb-f ">
            <h2 class="mil-fs-36">Contact info</h2>
            <div class="mil-dots"></div>
        </div>
        <div class="mil-features mil-bg-m-4 mil-br-md ">
            <div class="row p-20">
                <div class="col-lg-4 mil-tac flex justify-center items-center flex-col">
                    <img src="/img/ui/icons/13.png" alt="icon" class="mil-card-icon max-w-28 mb-10">
                    <h3 class="mil-fs-30 mil-mb-20 mil-lh-110">Write</h3>
                    <a href="mailto:{{ data.email }}" class="mil-fs-18 mil-mb-5" target="_blank">{{ data.email }}</a>
                </div>
                <div class="col-lg-4 mil-tac flex justify-center items-center flex-col">
                    <img src="/img/ui/icons/14.png" alt="icon" class="mil-card-icon max-w-28 mb-10">
                    <h3 class="mil-fs-30 mil-mb-20 mil-lh-110">Call</h3>
                    <a href="tel:{{ data.phoneNoSpaces }}" class="mil-fs-18 mil-mb-5" target="_blank">{{ data.phone }}</a>
                </div>
                <div class="col-lg-4 mil-tac flex justify-center items-center flex-col">
                    <img src="/img/ui/icons/15.png" alt="icon" class="mil-card-icon max-w-28 mb-10">
                    <h3 class="mil-fs-30 mil-mb-20 mil-lh-110">Come</h3>
                    <p class="mil-fs-18 mil-lh-160">71 South Los Carneros Road, <br>Goleta, California 93117</p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- contact info end -->