<div class="mil-top-panel">
    <div class="container">
        <div class="mil-tp-content">
            <div class="mil-left">
                <a href="/" id="#top" class="mil-logo mil-hover-bri-105" style="width: 20rem">
                    <img src="/img/ui/logo.png" alt="logo">
                        <img src="/img/ui/logo-light.png" alt="logo" class="mil-light"></a>
                    </div>
                    <nav class="mil-main-menu" id="mainMenu">
                        <ul id="swupMenu" class="mil-menu-transition mil-aic mil-m-4">
                            <li class="mil-has-children menu-item-wrapper services">
                                <a class="menu-title menu-link mil-has-children" href="/services">Services</a>
                                <ul>
                                    {% for item in nav.services.services %}
                                        <li>
                                            <a class="menu-link menu-link-child" href="{{ item.link }}">{{ item.title }}</a>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </li>
                            <li class="mil-has-children menu-item-wrapper service-areas">
                                <span class="menu-title menu-link mil-has-children">Service Ares</span>
                                <ul>
                                    {% for item in nav.locations.locations %}
                                        <li>
                                            <a class="menu-link menu-link-child" href="{{ item.link }}">{{ item.title }}</a>
                                        </li>
                                    {% endfor %}
                                </ul>
                            </li>
                            <li class="menu-item-wrapper contact">
                                <a class="menu-link" class="menu-title menu-link" href="{{ nav.contact.link }}">{{ nav.contact.title }}</a>
                            </li>
                        </ul>
                    </nav>
                    <div class="mil-right mil-jce">
                        <div class="mil-tp-phone mil-aic mil-mr-60 mil-sm-mr-15">
                            <i class="far fa-mobile mil-icon mil-sm mil-bg-a-2 mil-m-4 mil-mr-15 mil-sm-hidden" style="padding-top: .2rem"></i>
                            <a href="tel:{{ data.phoneNoSpaces }}" target="_blank" class="mil-fw-600" data-no-swup>{{ data.phone }}</a>
                        </div>
                        <a
                            href="/quote"
                            class="mil-btn mil-lg mil-bg-a-1 mil-br-xl mil-hover-bri-105 mil-icon-btn mil-hover-scale mil-md-hidden">
                            <span>{{ data.quoteText }}</span>
                            <i class="far fa-magic mil-bg-m-1 mil-a-1"></i>
                        </a>
                        <div class="mil-menu-btn">
                            <span></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>